$(document).ready(function(){
	
	getNote();
	
});

function getNote(){
	$.ajax({
		type:"POST" , 
		url:"./../getNote",
		contentType:"text/plain",
		data:$("#id").val()+"",
		success : function(data){
			$("#note_title").val(data.title);
			$("#note_detail").val(data.detail)

			alert(data.title);
		},error:function(data){
			alert(data);
		}
	});
}